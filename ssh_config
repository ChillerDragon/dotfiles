# version 0008
# for IdentitiesOnly yes see:
# https://stackoverflow.com/a/11251797

Include config.d/*

Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa
    IdentitiesOnly yes
Host github-WORK
    HostName github.com
    User git
    IdentityFile ~/.ssh/work_rsa
    IdentitiesOnly yes
Host gitlab-MIRROR
    HostName gitlab.com
    User git
    IdentityFile ~/.ssh/ChillerMirror/id_rsa
    IdentitiesOnly yes
Host github-ZillyHuhn
    HostName github.com
    User git
    IdentityFile ~/.ssh/ZillyHuhn/id_rsa
    IdentitiesOnly yes
Host aur.archlinux.org
    IdentityFile ~/.ssh/chiller@debian/id_rsa
    User aur
